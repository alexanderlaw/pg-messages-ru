#. +> 17 16 15 14 13 12
#: pg_archivecleanup.c:260
#, c-format
msgid "Usage:\n"
msgstr "Использование:\n"

#. +> 17 16 15 14 13 12
#: pg_archivecleanup.c:261
#, c-format
msgid "  %s [OPTION]... ARCHIVELOCATION OLDESTKEPTWALFILE\n"
msgstr ""
"  %s [ПАРАМЕТР]... РАСПОЛОЖЕНИЕ_АРХИВА СТАРЕЙШИЙ_СОХРАНЯЕМЫЙ_ФАЙЛ_WAL\n"

