"\n"

#. +> 17 16 15 14 13 12
#: pg_ctl.c:1964
#, c-format
msgid "Usage:\n"
msgstr "Использование:\n"

#. +> 17 16 15 14 13 12
#: pg_ctl.c:1965
#, c-format
msgid "  %s init[db]   [-D DATADIR] [-s] [-o OPTIONS]\n"
msgstr "  %s init[db]   [-D КАТАЛОГ-ДАННЫХ] [-s] [-o ПАРАМЕТРЫ]\n"

#. +> 17 16 15 14 13 12
#: pg_ctl.c:1966
#, c-format
msgid ""
"  %s start      [-D DATADIR] [-l FILENAME] [-W] [-t SECS] [-s]\n"
"                    [-o OPTIONS] [-p PATH] [-c]\n"
msgstr ""
"  %s start      [-D КАТАЛОГ-ДАННЫХ] [-l ИМЯ-ФАЙЛА] [-W] [-t СЕК] [-s]\n"
"                    [-o ПАРАМЕТРЫ] [-p ПУТЬ] [-c]\n"

#. +> 17 16 15 14 13 12
#: pg_ctl.c:1968
#, c-format
msgid "  %s stop       [-D DATADIR] [-m SHUTDOWN-MODE] [-W] [-t SECS] [-s]\n"
msgstr ""
"  %s stop       [-D КАТАЛОГ-ДАННЫХ] [-m РЕЖИМ-ОСТАНОВКИ] [-W] [-t СЕК] [-s]\n"

#. +> 17 16 15 14 13 12
#: pg_ctl.c:1969
#, c-format
msgid ""
"  %s restart    [-D DATADIR] [-m SHUTDOWN-MODE] [-W] [-t SECS] [-s]\n"
"                    [-o OPTIONS] [-c]\n"
msgstr ""
"  %s restart    [-D КАТАЛОГ-ДАННЫХ] [-m РЕЖИМ-ОСТАНОВКИ] [-W] [-t СЕК] [-s]\n"
"                    [-o ПАРАМЕТРЫ] [-c]\n"

#. +> 17 16 15 14 13 12
#: pg_ctl.c:1971
#, c-format
msgid "  %s reload     [-D DATADIR] [-s]\n"
msgstr "  %s reload     [-D КАТАЛОГ-ДАННЫХ] [-s]\n"

#. +> 17 16 15 14 13 12
#: pg_ctl.c:1972
#, c-format
msgid "  %s status     [-D DATADIR]\n"
msgstr "  %s status     [-D КАТАЛОГ-ДАННЫХ]\n"

#. +> 17 16 15 14 13 12
#: pg_ctl.c:1973
#, c-format
msgid "  %s promote    [-D DATADIR] [-W] [-t SECS] [-s]\n"
msgstr "  %s promote    [-D КАТАЛОГ-ДАННЫХ] [-W] [-t СЕК] [-s]\n"

#. +> 17 16 15 14 13 12
#: pg_ctl.c:1974
#, c-format
msgid "  %s logrotate  [-D DATADIR] [-s]\n"
msgstr "  %s logrotate  [-D КАТАЛОГ-ДАННЫХ] [-s]\n"

#. +> 17 16 15 14 13 12
#: pg_ctl.c:1975
#, c-format
msgid "  %s kill       SIGNALNAME PID\n"
msgstr "  %s kill       СИГНАЛ PID\n"

